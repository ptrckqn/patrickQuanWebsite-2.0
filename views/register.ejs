<% include partials/header %>

<div class="container">
  <h2 class="pageTitle">Register</h2>
</div>
<div class="container contactFormContainer">
  <p>Register a new admin account</p>
  <p>
    <form class="contactForm" action="/register" method="post">
      <input type="text" name="username" placeholder="Username" required>
      <input type="password" name="password" placeholder="Password" required>
      <div>
        <button type="submit" class="sendButton">SIGN UP</button>
      </div>
    </form>
  </p>
</div>

